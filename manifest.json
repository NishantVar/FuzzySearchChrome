{
	"manifest_version": 2,
	"name": "Fuzzy Search",
	"version": "1.0",

	"background": {
	    "persistent": true,
	    "scripts": [
	    	"search.js",
	    	"store.js",
	    	"bg.js"
	    ]
	},

	"permissions": [
		"bookmarks",
		"history"
	],

	"description": "Nothing",
	"browser_action": {
		"default_icon": "icon.png",
		"default_popup": "popup.html"
	},
	"omnibox": { "keyword": "z" },
	"icons": {
		"16": "icon.png"
	}
}